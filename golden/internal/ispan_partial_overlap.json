{
  "name": "ispan_partial_overlap",
  "description": "Vcopy subset of fragmented content - reveals I-span granularity",
  "operations": [
    {
      "op": "create_document",
      "doc": "source",
      "result": "1.1.0.1.0.1"
    },
    {
      "op": "insert_loop",
      "count": 10,
      "each": "A-J"
    },
    {
      "op": "create_document",
      "doc": "dest",
      "result": "1.1.0.1.0.2"
    },
    {
      "op": "vcopy",
      "from": "positions 3-7 (CDEFG)",
      "to": "dest"
    },
    {
      "op": "retrieve_contents",
      "source": [
        "CDEFG"
      ],
      "dest": [
        "CDEFG"
      ],
      "expected": "CDEFG in both"
    },
    {
      "op": "compare_versions",
      "shared_span_pairs": 1,
      "shared": [
        {
          "source": {
            "start": "1.3",
            "width": "0.5"
          },
          "dest": {
            "start": "1.1",
            "width": "0.5"
          }
        }
      ],
      "interpretation": "1 pair = consolidated, 5 pairs = per-insert granularity"
    }
  ]
}